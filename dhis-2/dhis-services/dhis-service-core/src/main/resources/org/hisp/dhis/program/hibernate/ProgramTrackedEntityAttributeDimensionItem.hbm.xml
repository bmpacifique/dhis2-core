<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">]
  >

<hibernate-mapping>
  <class name="org.hisp.dhis.program.ProgramTrackedEntityAttributeDimensionItem" table="programtrackedentityattributedimensionitem">

    <id name="id" column="programtrackedentityattributedimensionitemid">
      <generator class="native" />
    </id>

    <many-to-one name="program" class="org.hisp.dhis.program.Program"
      column="programid" foreign-key="fk_programattributedimensionitem_programid" not-null="true" />

    <many-to-one name="attribute" class="org.hisp.dhis.trackedentity.TrackedEntityAttribute"
      column="trackedentityattributeid" foreign-key="fk_programattributedimensionitem_attributeid" not-null="true" />

  </class>

</hibernate-mapping>
